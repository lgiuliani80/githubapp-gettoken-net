{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "appServicePlanSkuName": {
      "value": "${AZURE_APP_SERVICE_PLAN_SKU_NAME=S1}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "principalType": {
      "value": "${AZURE_PRINCIPAL_TYPE}"
    },
    "githubAppClientId": {
      "value": "${GITHUB_APP_CLIENT_ID=}"
    },
    "githubPrivateKeySecretName": {
      "value": "${GITHUB_PRIVATE_KEY_SECRET_NAME=github-private-key}"
    },
    "entraTenantId": {
      "value": "${AZURE_TENANT_ID}"
    },
    "entraClientId": {
      "value": "${ENTRA_CLIENT_ID=}"
    },
    "entraDomain": {
      "value": "${ENTRA_DOMAIN=}"
    },
    "validAudiences": {
      "value": "${ENTRA_VALID_AUDIENCES=}"
    },
    "requireAuthentication": {
      "value": "${REQUIRE_AUTHENTICATION=true}"
    },
    "mapOpenApi": {
      "value": "${MAP_OPENAPI=false}"
    }
  }
}
